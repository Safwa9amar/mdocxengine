{
  "name": "mdocxengine",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && vite build --ssr",
    "test": "vitest --ui --coverage",
    "test:ci": "vitest run --coverage",
    "prepublishOnly": "npm run build --omit=dev",
    "lint": "eslint --flag unstable_ts_config --config eslint.config.ts",
    "predemo": "npm run build",
    "demo": "tsx ./demo/index.ts",
    "typedoc": "typedoc src/index.ts --tsconfig tsconfig.typedoc.json",
    "prettier": "prettier -l \"{src,scripts,demo}/**/*.{ts,html}\"",
    "prettier:fix": "npm run prettier -- --write",
    "cspell": "cspell \"{src,demo,docs,scripts}/**/*.{ts,scss,html,md}\" && cspell \"./*.*\"",
    "serve.docs": "cd docs && docsify serve",
    "extract": "tsx scripts/extract-document.ts",
    "run-ts": "tsx"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "adm-zip": "^0.5.16",
    "module-alias": "^2.2.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prettier-plugin-xml": "^0.1.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
